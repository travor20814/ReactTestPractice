{
 "presets": [
   ["@babel/preset-env", {
     "targets": {
       "browsers": ["last 2 versions"],
       "node": "current",
     },
   }],
   "@babel/react",
 ],
 "plugins": [
   "@babel/plugin-transform-runtime",
   "@babel/transform-flow-strip-types",
   "@babel/plugin-syntax-dynamic-import",
   "react-hot-loader/babel",
 ],
}
